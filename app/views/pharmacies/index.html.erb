<%- model_class = Pharmacy -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:street_name) %></th>
      <th><%= model_class.human_attribute_name(:house_number) %></th>
      <th><%= model_class.human_attribute_name(:postal_code) %></th>
      <th><%= model_class.human_attribute_name(:cent_cost_per_phone_call) %></th>
      <th><%= model_class.human_attribute_name(:fax_number) %></th>
      <th><%= model_class.human_attribute_name(:email) %></th>
      <th><%= model_class.human_attribute_name(:website) %></th>
      <th><%= model_class.human_attribute_name(:service_number) %></th>
      <th><%= model_class.human_attribute_name(:facebook_profile) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @pharmacies.each do |pharmacy| %>
      <tr>
        <td><%= link_to pharmacy.id, pharmacy_path(pharmacy) %></td>
        <td><%= pharmacy.name %></td>
        <td><%= pharmacy.street_name %></td>
        <td><%= pharmacy.house_number %></td>
        <td><%= pharmacy.postal_code %></td>
        <td><%= pharmacy.cent_cost_per_phone_call %></td>
        <td><%= pharmacy.fax_number %></td>
        <td><%= pharmacy.email %></td>
        <td><%= pharmacy.website %></td>
        <td><%= pharmacy.service_number %></td>
        <td><%= pharmacy.facebook_profile %></td>
        <td><%=l pharmacy.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      pharmacy_path(pharmacy), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_pharmacy_path(pharmacy), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      pharmacy_path(pharmacy),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_pharmacy_path,
            :class => 'btn btn-primary' %>
